{
  "name": "public-web",
  "version": "1.0.0",
  "description": "Advanced boilerplate and sample app with Lit Element, Redux, Redux-Saga, Reselect, SCSS.",
  "author": "Dzintars Klavins",
  "license": "ISC",
  "homepage": "https://github.com/oswee/public-web#readme",
  "keywords": [],
  "main": "index.js",
  "private": true,
  "bugs": {
    "url": "https://github.com/oswee/public-web/issues"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --open",
    "build": "webpack --config webpack.prod.js",
    "lint": "tslint -c tslint.json 'src/**/*.ts' > tslint.log",
    "lint:eslint": "eslint --ext .js,.html .",
    "lint:prettier": "prettier \"**/*.js\" --list-different || (echo '↑↑ these files are not prettier formatted ↑↑' && exit 1)",
    "lint:types": "tsc",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint --ext .js,.html . --fix",
    "format:prettier": "prettier \"**/*.js\" --write",
    "storybook": "start-storybook -p 9002",
    "storybook:build": "build-storybook -o _site -s storybook-static",
    "site:build": "npm run storybook:build"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oswee/public-web.git"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.4.0",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "lit-html-element": "^0.9.1",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-transform-typescript": "^7.7.4",
    "@open-wc/demoing-storybook": "^1.3.4",
    "@open-wc/eslint-config": "^2.0.1",
    "@open-wc/prettier-config": "^0.1.13",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-backgrounds": "^5.2.8",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-notes": "^5.2.8",
    "@storybook/addon-storysource": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/polymer": "^5.2.8",
    "@storybook/preset-typescript": "^1.1.0",
    "@storybook/react": "^5.2.8",
    "@storybook/theming": "^5.2.8",
    "@types/node": "^12.12.17",
    "@types/webpack": "^4.41.0",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "@vaadin/router": "^1.5.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.3.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-lit": "^1.2.0",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "polymer-webpack-loader": "^2.0.3",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "webpack-pwa-manifest": "^4.1.1",
    "workbox-webpack-plugin": "^4.3.1"
  }
}
